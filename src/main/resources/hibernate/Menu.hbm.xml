<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.yoda.menu.model.Menu" table="menu" lazy="false">
		<comment></comment>
		<id name="menuId" type="long">
			<column name="menu_id" />
			<generator class="increment" />
		</id>
		<property name="siteId" type="long">
			<column name="site_id">
				<comment />
			</column>
		</property>
		<property name="menuSetName" type="string">
			<column name="menu_set_name" length="20" not-null="true">
				<comment></comment>
			</column>
		</property>
		<property name="menuTitle" type="string">
			<column name="menu_title" length="60" not-null="true">
				<comment></comment>
			</column>
		</property>
		<property name="menuName" type="string">
			<column name="menu_name" length="60" not-null="true">
				<comment></comment>
			</column>
		</property>
		<property name="seqNum" type="int">
			<column name="seq_num" not-null="true">
				<comment></comment>
			</column>
		</property>
		<property name="menuParentId" type="long">
			<column name="menu_parent_id" not-null="false">
				<comment></comment>
			</column>
		</property>
		<property name="menuType" type="string">
			<column name="menu_type" length="5" not-null="true">
				<comment></comment>
			</column>
		</property>
		<property name="menuUrl" type="string">
			<column name="menu_url" length="255" not-null="true">
				<comment></comment>
			</column>
		</property>
		<property name="menuWindowTarget" type="string">
			<column name="menu_window_target" length="20" not-null="true">
				<comment></comment>
			</column>
		</property>
		<property name="menuWindowMode" type="string">
			<column name="menu_window_mode" length="255" not-null="true">
				<comment></comment>
			</column>
		</property>
		<property name="published" type="char">
			<column name="published" length="1" not-null="true">
				<comment></comment>
			</column>
		</property>
		<property name="recUpdateBy" type="long">
			<column name="rec_update_by" not-null="true">
				<comment></comment>
			</column>
		</property>
		<property name="recUpdateDatetime" type="timestamp">
			<column name="rec_update_datetime" not-null="true">
				<comment></comment>
			</column>
		</property>
		<property name="recCreateBy" type="long">
			<column name="rec_create_by" not-null="true">
				<comment></comment>
			</column>
		</property>
		<property name="recCreateDatetime" type="timestamp">
			<column name="rec_create_datetime" not-null="true">
				<comment></comment>
			</column>
		</property>
		<many-to-one name="section" column="section_id" class="com.yoda.section.model.Section" />
		<!-- <many-to-one name="item" column="item_id" class="com.yoda.controlpanel.item.model.Item" /> -->
		<many-to-one name="content" column="content_id" class="com.yoda.content.model.Content" />
	</class>
</hibernate-mapping>