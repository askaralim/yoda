<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.yoda.poll.model.PollHeader" table="poll_header">
		<comment />
		<id name="pollHeaderId" type="java.lang.Long" unsaved-value="0">
			<column name="poll_header_id" />
			<generator class="increment" />
		</id>
		<property name="siteId" type="string">
			<column name="site_id" length="20" not-null="true">
				<comment />
			</column>
		</property>
		<property name="pollTopic" type="string">
			<column name="poll_topic" length="255" not-null="true">
				<comment />
			</column>
		</property>
		<property name="pollPublishOn" type="date">
			<column name="poll_publish_on" length="10" not-null="true">
				<comment></comment>
			</column>
		</property>
		<property name="pollExpireOn" type="date">
			<column name="poll_expire_on" length="10" not-null="true">
				<comment></comment>
			</column>
		</property>
		<property name="published" type="char">
			<column name="published" length="1" not-null="true">
				<comment></comment>
			</column>
		</property>
		<property name="recUpdateBy" type="string">
			<column name="rec_update_by" length="20" not-null="true">
				<comment />
			</column>
		</property>
		<property name="recUpdateDatetime" type="timestamp">
			<column name="rec_update_datetime" not-null="true">
				<comment />
			</column>
		</property>
		<property name="recCreateBy" type="string">
			<column name="rec_create_by" length="20" not-null="true">
				<comment />
			</column>
		</property>
		<property name="recCreateDatetime" type="timestamp">
			<column name="rec_create_datetime" not-null="true">
				<comment />
			</column>
		</property>
		<set name="pollDetails" inverse="false" order-by="seq_num">
			<key column="poll_header_id" />
			<one-to-many class="com.yoda.poll.model.PollDetail" />
		</set>
	</class>
</hibernate-mapping>